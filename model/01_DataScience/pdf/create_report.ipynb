   "source": [
    "import os\n",
    "from PyPDF2 import PdfMerger\n",
    "\n",
    "merger = PdfMerger()\n",
    "\n",
    "for file in sorted(os.listdir(\"./\")):\n",
    "    if file.endswith(\".pdf\"):\n",
    "        merger.append(os.path.join(\"./\", file))\n",
    "\n",
    "merger.write(\"Priloha_1_Data_Science.pdf\")\n",
    "merger.close()"
   ]
  }
